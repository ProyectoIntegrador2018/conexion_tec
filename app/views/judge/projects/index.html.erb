<p id="notice"><%= notice %></p>

<h1>Projects</h1>

<table>
  <thead>
    <tr>
      <th>Name</th>
      <th>Field</th>
      <th>Kind</th>
      <th>Abstract</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @projects.each do |project| %>
      <tr>
        <td><%= project.name %></td>
        <td><%= project.field %></td>
        <td><%= project.kind %></td>
        <td><%= project.abstract %></td>
      </tr>
    <% end %>
  </tbody>
</table>